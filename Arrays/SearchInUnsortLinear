//Searching in unsorted linear array

public class UnsortLinear {
    static int FindNumber(int arr[],int n,int key){
        for (int i=0;i<n;i++) {
            if (arr[i] == key)
                return i;
        }
        return -1;
    }

    public static void main(String[] args) {
        int arr[]={2,4,6,8,5,1};
        int n=arr.length;
        int key=8;
       int x= FindNumber(arr,n,key);
       if (x== -1)
        System.out.println("Element not in the array");
       else
           System.out.println("element present"+ x);
    }
}

